{
  "name": "tanstack-dashboard",
  "private": true,
  "version": "1.5.0",
  "type": "module",
  "scripts": {
    "build": "bun run type-check && bun --bun vite build",
    "preview": "bun --bun vite preview",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "type-check:node": "tsc --project tsconfig.node.json --noEmit",
    "type-check:all": "bun run type-check && bun run type-check:node",
    "lint": "biome lint --write",
    "check": "biome check --write",
    "check:ci": "biome check",
    "format": "biome format --write",
    "dev": "concurrently \"bun run client\" \"bun run server\"",
    "client": "bun --bun vite",
    "server": "cd ./backend && bun run dev",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=22.19.0"
  },
